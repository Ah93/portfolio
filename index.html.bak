        :root {
            --primary-gradient: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            --dark-gradient: linear-gradient(135deg, #1e3c72 0%, #2a5298 100%);
            --text-gradient: linear-gradient(45deg, #667eea, #764ba2);
            --dark-text-gradient: linear-gradient(45deg, #60a5fa, #a78bfa);
            --transition-fast: all 0.3s ease;
            --transition-smooth: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
        }

        * {
            font-family: 'Inter', sans-serif;
        }

        /* Theme Variables */
        [data-theme="light"] {
            --bg-primary: #f8f9fa;
            --bg-secondary: #ffffff;
            --text-primary: #212529;
            --text-secondary: #6c757d;
            --border-color: #dee2e6;
            --shadow: rgba(0,0,0,0.1);
        }

        [data-theme="dark"] {
            --bg-primary: #1a1a1a;
            --bg-secondary: #2d2d2d;
            --text-primary: #ffffff;
            --text-secondary: #adb5bd;
            --border-color: #495057;
            --shadow: rgba(255,255,255,0.1);
        }

        body {
            background-color: var(--bg-primary);
            color: var(--text-primary);
            transition: var(--transition-fast);
        }

        /* Utilities */
        .gradient-bg {
            background: var(--primary-gradient);
        }

        .dark-gradient-bg {
            background: var(--dark-gradient);
        }

        .hero-text {
            background: var(--text-gradient);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        [data-theme="dark"] .hero-text {
            background: var(--dark-text-gradient);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        .card-custom {
            background-color: var(--bg-secondary);
            border: 1px solid var(--border-color);
            transition: var(--transition-fast);
        }

        .card-hover:hover {
            transform: translateY(-10px);
            box-shadow: 0 20px 40px var(--shadow);
        }

        .project-card {
            transition: var(--transition-smooth);
        }

        .project-card:hover {
            transform: scale(1.05) rotateY(5deg);
        }

        /* Animations */
        .animate-float {
            animation: float 6s ease-in-out infinite;
        }

        @keyframes float {
            0%, 100% { transform: translateY(0px); }
            50% { transform: translateY(-20px); }
        }

        .fade-in {
            opacity: 0;
            transform: translateY(30px);
            animation: fadeIn 0.8s ease forwards;
        }

        @keyframes fadeIn {
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        /* Navigation */
        .navbar-custom {
            background-color: rgba(var(--bg-secondary-rgb), 0.9);
            backdrop-filter: blur(10px);
            border-bottom: 1px solid var(--border-color);
        }

        .nav-link-custom {
            color: var(--text-secondary);
            transition: var(--transition-fast);
            padding: 0.5rem 1rem;
            border-radius: 0.5rem;
            margin: 0 0.25rem;
        }

        .nav-link-custom:hover {
            color: #667eea;
        }

        .nav-link-custom.active {
            background: var(--primary-gradient);
            color: white !important;
        }

        /* Buttons */
        .btn-gradient {
            background: var(--primary-gradient);
            border: none;
            color: white;
            transition: var(--transition-fast);
        }

        .btn-gradient:hover {
            color: white;
            transform: translateY(-2px);
            box-shadow: 0 10px 25px rgba(102, 126, 234, 0.3);
        }

        .btn-outline-gradient {
            border: 2px solid #667eea;
            color: #667eea;
            background: transparent;
            transition: var(--transition-fast);
        }

        .btn-outline-gradient:hover {
            background: #667eea;
            color: white;
        }

        /* Mobile Navigation */
        .mobile-nav {
            position: fixed;
            bottom: 1rem;
            left: 50%;
            transform: translateX(-50%);
            z-index: 1000;
            background: var(--bg-secondary);
            border-radius: 2rem;
            padding: 0.5rem;
            box-shadow: 0 10px 30px var(--shadow);
        }

        .mobile-nav-btn {
            width: 3rem;
            height: 3rem;
            border-radius: 50%;
            border: none;
            background: transparent;
            color: var(--text-secondary);
            transition: var(--transition-fast);
            margin: 0 0.25rem;
        }

        .mobile-nav-btn.active {
            background: var(--primary-gradient);
            color: white;
        }

        /* Section Spacing */
        .section {
            min-height: 100vh;
            padding-top: 100px;
            display: none;
        }

        .section.active {
            display: block;
        }

        /* RTL Support */
        [dir="rtl"] .ms-3 { margin-left: 0 !important; margin-right: 1rem !important; }
        [dir="rtl"] .me-3 { margin-right: 0 !important; margin-left: 1rem !important; }
        [dir="rtl"] .text-start { text-align: right !important; }
        [dir="rtl"] .text-end { text-align: left !important; }

        /* Dark theme specific styles */
        [data-theme="dark"] {
            --bg-secondary-rgb: 45, 45, 45;
        }

        [data-theme="light"] {
            --bg-secondary-rgb: 255, 255, 255;
        }

        /* Responsive adjustments */
        @media (max-width: 768px) {
            .hero-text {
                font-size: 2.5rem !important;
            }
            
            .display-1 {
                font-size: 2rem !important;
            }
		
        }
    </style>
